<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity7">

    <TextView
            android:text="Cancel Booking"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/CancelBookingText"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="96dp"/>

    <TextView
            android:layout_width="129dp"
            android:layout_height="23dp" android:id="@+id/MyBooking1"
            app:layout_constraintEnd_toEndOf="@+id/CancelBookingText"
            app:layout_constraintStart_toStartOf="@+id/CancelBookingText"
            app:layout_constraintTop_toBottomOf="@+id/CancelBookingText" android:layout_marginTop="40dp"/>
    <TextView
            android:layout_width="129dp"
            android:layout_height="24dp" android:id="@+id/viewBookingtext2"
            app:layout_constraintEnd_toEndOf="@+id/MyBooking1"
            app:layout_constraintStart_toStartOf="@+id/MyBooking1"
            app:layout_constraintTop_toBottomOf="@+id/MyBooking1"/>
    <Button
            android:text="Main Menu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/ReturnToMenuButton1"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="314dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <Button
            android:text="Cancel Booking"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/CancelBookingBt"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/ReturnToMenuButton1" app:layout_constraintHorizontal_bias="0.498"
            android:layout_marginBottom="16dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>